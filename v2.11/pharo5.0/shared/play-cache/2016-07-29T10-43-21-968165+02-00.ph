| v c ce c2 ce2 c3 ce3 c4 ce4 el|v := RWView new.v scene backgroundColor: WDColor white .c := RWCube new.c depth: 0.001.c height: 0.1.c width: 10.c color: WDColor black.ce := c element translateByY: 0.45.c3 := RWCube new.c3 depth: 0.001.c3 height: 0.1.c3 color: WDColor black.ce3 := c3 element translateByY: -0.95.c2 := RWCube new.c2 depth: 0.001.c2 height: 3.c2 width: 0.1.c2 color: WDColor blue.ce2 := c2 element translateByX: -4.95; translateByY: -7.c4 := RWCube new.c4 depth: 0.001.c4 height: 1.5.c4 width: 0.1.c4 color: WDColor black.ce4 := c4 element translateByX: 0.45; translateByY: -0.25.v add: ce.v add: ce2.v add: ce3.v add: ce4.v camera position: (WDVector3 x: 0.0 y: 0.0 z: 10.0).v @ RWMouseKeyControl.v open.
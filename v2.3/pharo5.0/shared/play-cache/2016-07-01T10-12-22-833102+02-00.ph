| t7 codeCompo |codeCompo := 'Descriptor PrinterPH5 extends Component {	provides {		default : {			print(txt : String by require);			main();		}	}		service main() {		self.print(''Printer PH5 test print'');	}		service print(txt : String by require) {		Transcript.crShow(txt);	}}'.codeCompo := 'Descriptor ContainerPH5 extends Component {	provides {		default : {			main();		}	}		internally requires {		printer : PrinterPH5;	}		architecture {		connect printer to default@(PrinterPH5.new()); 		}		service main() {		printer.print(''Contener PH5 test print'');	}	}'.t7 := CompoToSmalltalk convert: codeCompo.Compiler evaluate: t7.						